{
  "dashboard": {
    "title": "vLLM Gateway Monitoring",
    "tags": ["vllm", "inference", "llm"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Time to First Token (TTFT)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(gateway_time_to_first_token_seconds_bucket[1m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(gateway_time_to_first_token_seconds_bucket[1m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(gateway_time_to_first_token_seconds_bucket[1m])) by (le))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Time",
            "logBase": 1,
            "show": true
          },
          {
            "format": "short",
            "logBase": 1,
            "show": true
          }
        ],
        "xaxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        },
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        }
      },
      {
        "id": 2,
        "title": "Tokens per Second",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(gateway_tokens_per_second_bucket[1m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(gateway_tokens_per_second_bucket[1m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(gateway_tokens_per_second_bucket[1m])) by (le))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Tokens/sec",
            "logBase": 1,
            "show": true
          },
          {
            "format": "short",
            "logBase": 1,
            "show": true
          }
        ],
        "xaxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        },
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        }
      },
      {
        "id": 3,
        "title": "Requests per Second",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(gateway_requests_total{route=\"/chat\"}[1m])",
            "legendFormat": "RPS",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec",
            "logBase": 1,
            "show": true
          },
          {
            "format": "short",
            "logBase": 1,
            "show": true
          }
        ],
        "xaxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        },
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        }
      },
      {
        "id": 4,
        "title": "Queue Depth (In-Flight Requests)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "gateway_queue_depth",
            "legendFormat": "Queue Depth",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Requests",
            "logBase": 1,
            "min": 0,
            "show": true
          },
          {
            "format": "short",
            "logBase": 1,
            "show": true
          }
        ],
        "xaxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        },
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "max": true,
          "avg": true
        }
      },
      {
        "id": 5,
        "title": "GPU Utilization (%)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "nvidia_gpu_duty_cycle{uuid=\"GPU-c92e3220-60c9-ba46-4867-5e4c1fdca390\"}",
            "legendFormat": "GPU 0 (RTX 3090)",
            "refId": "A"
          },
          {
            "expr": "nvidia_gpu_duty_cycle{uuid=\"GPU-f78f6523-21d0-de20-7387-7706a8a9698b\"}",
            "legendFormat": "GPU 1 (RTX 3090)",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Utilization",
            "logBase": 1,
            "min": 0,
            "max": 100,
            "show": true
          },
          {
            "format": "short",
            "logBase": 1,
            "show": true
          }
        ],
        "xaxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        },
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "max": true,
          "avg": true
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [90],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "GPU Utilization Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 6,
        "title": "GPU Memory Usage",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "nvidia_gpu_memory_used_bytes{uuid=\"GPU-c92e3220-60c9-ba46-4867-5e4c1fdca390\"} / nvidia_gpu_memory_total_bytes{uuid=\"GPU-c92e3220-60c9-ba46-4867-5e4c1fdca390\"} * 100",
            "legendFormat": "GPU 0 Memory %",
            "refId": "A"
          },
          {
            "expr": "nvidia_gpu_memory_used_bytes{uuid=\"GPU-f78f6523-21d0-de20-7387-7706a8a9698b\"} / nvidia_gpu_memory_total_bytes{uuid=\"GPU-f78f6523-21d0-de20-7387-7706a8a9698b\"} * 100",
            "legendFormat": "GPU 1 Memory %",
            "refId": "B"
          },
          {
            "expr": "nvidia_gpu_memory_used_bytes{uuid=\"GPU-c92e3220-60c9-ba46-4867-5e4c1fdca390\"} / 1024 / 1024 / 1024",
            "legendFormat": "GPU 0 Used (GB)",
            "refId": "C"
          },
          {
            "expr": "nvidia_gpu_memory_used_bytes{uuid=\"GPU-f78f6523-21d0-de20-7387-7706a8a9698b\"} / 1024 / 1024 / 1024",
            "legendFormat": "GPU 1 Used (GB)",
            "refId": "D"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Memory %",
            "logBase": 1,
            "min": 0,
            "max": 100,
            "show": true
          },
          {
            "format": "decgbytes",
            "label": "Memory (GB)",
            "logBase": 1,
            "show": true
          }
        ],
        "xaxis": {
          "show": true,
          "mode": "time"
        },
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "nullPointMode": "null",
        "tooltip": {
          "shared": true,
          "sort": 0,
          "value_type": "individual"
        },
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "max": true,
          "avg": true
        }
      },
      {
        "id": 7,
        "title": "Total Requests",
        "type": "stat",
        "gridPos": {"x": 0, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(gateway_requests_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0
          }
        }
      },
      {
        "id": 8,
        "title": "Average TTFT (Last 5m)",
        "type": "stat",
        "gridPos": {"x": 6, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(gateway_time_to_first_token_seconds_sum[5m]) / rate(gateway_time_to_first_token_seconds_count[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 1.0, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Average Throughput (Last 5m)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(gateway_tokens_per_second_sum[5m]) / rate(gateway_tokens_per_second_count[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Current Queue Depth",
        "type": "stat",
        "gridPos": {"x": 18, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "gateway_queue_depth",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(gateway_requests_total[1m]) > 0",
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Load Test Events",
          "tagKeys": "load_test",
          "textFormat": "Load change detected",
          "titleFormat": "Traffic Change"
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 1,
    "links": []
  },
  "overwrite": true
}

