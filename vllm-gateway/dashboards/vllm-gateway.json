{
  "title": "vLLM Gateway Monitoring",
  "tags": ["vllm", "inference", "llm"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "Time to First Token (TTFT)",
      "type": "timeseries",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(gateway_time_to_first_token_seconds_bucket[1m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(gateway_time_to_first_token_seconds_bucket[1m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(gateway_time_to_first_token_seconds_bucket[1m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      },
      "options": {
        "legend": {"showLegend": true, "displayMode": "list"}
      }
    },
    {
      "id": 2,
      "title": "Tokens per Second",
      "type": "timeseries",
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(gateway_tokens_per_second_bucket[1m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(gateway_tokens_per_second_bucket[1m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Requests per Second",
      "type": "timeseries",
      "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "rate(gateway_requests_total{route=\"/chat\"}[1m])",
          "legendFormat": "RPS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Queue Depth (In-Flight Requests)",
      "type": "timeseries",
      "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "gateway_queue_depth",
          "legendFormat": "Queue Depth",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 5,
      "title": "GPU Utilization (%)",
      "type": "timeseries",
      "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "nvidia_gpu_duty_cycle",
          "legendFormat": "{{uuid}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 6,
      "title": "GPU Memory Usage (%)",
      "type": "timeseries",
      "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "nvidia_gpu_memory_used_bytes / nvidia_gpu_memory_total_bytes * 100",
          "legendFormat": "{{uuid}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 7,
      "title": "Total Requests",
      "type": "stat",
      "gridPos": {"x": 0, "y": 24, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "sum(gateway_requests_total)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "id": 8,
      "title": "Avg TTFT (5m)",
      "type": "stat",
      "gridPos": {"x": 6, "y": 24, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "rate(gateway_time_to_first_token_seconds_sum[5m]) / rate(gateway_time_to_first_token_seconds_count[5m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 0.5, "color": "yellow"},
              {"value": 1.0, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "id": 9,
      "title": "Avg Tokens/sec (5m)",
      "type": "stat",
      "gridPos": {"x": 12, "y": 24, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "rate(gateway_tokens_per_second_sum[5m]) / rate(gateway_tokens_per_second_count[5m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 50, "color": "yellow"},
              {"value": 100, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    },
    {
      "id": 10,
      "title": "Queue Depth",
      "type": "stat",
      "gridPos": {"x": 18, "y": 24, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "gateway_queue_depth",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 10, "color": "yellow"},
              {"value": 50, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      }
    }
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
  },
  "editable": true,
  "graphTooltip": 1
}